<%- include ("header") -%>



    <div class="box" id="heading">
       <h1>
            <%= title %>
       </h1>
        <h1>
            <%= listTitle %>
        </h1>
    </div>

    <div class="box">
        <% for (var item in todo_list) { %>

            <div class="item">
                <input type="checkbox">
                <p>
                    <%= todo_list[item] %>
                </p>

            </div>
            <% } %>


                <form class="item" action="/" method="POST">
                    <input type="text" name="todo" placeholder="New TODO" autocomplete="off">
                    <button type="submit" name="list" value=<%= listTitle %>>+</button>
                </form>
               

    </div>

    <%- include ("footer") -%>


